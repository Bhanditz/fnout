{
  "name": "fnout",
  "version": "1.0.0",
  "description": "Identify the contents of files, in particular, executables",
  "main": "index.js",
  "scripts": {
    "test": "npm run tape && npm run lint",
    "tape": "node test/index.js | tap-difflet",
    "lint": "standard 'src/*.js'",
    "cov": "nyc -s node test/index.js | tap-difflet && nyc report -r text",
    "coveralls": "nyc -s node test/index.js | tap-difflet && nyc report -r text-lcov | coveralls",
    "compile": "babel -d lib/ src/",
    "prepublish": "npm run compile"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itchio/fnout.git"
  },
  "keywords": [
    "sniff",
    "filesystem"
  ],
  "author": "Amos Wenger <amos@itch.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/itchio/fnout/issues"
  },
  "homepage": "https://github.com/itchio/fnout#readme",
  "devDependencies": {
    "assert": "^1.3.0",
    "babel-eslint": "^6.0.2",
    "babel-plugin-syntax-async-functions": "^6.5.0",
    "babel-plugin-transform-async-to-module-method": "^6.7.0",
    "babel-plugin-transform-es2015-destructuring": "^6.6.5",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.7.4",
    "babel-plugin-transform-es2015-parameters": "^6.7.0",
    "babel-plugin-transform-strict-mode": "^6.6.5",
    "babel-register": "^6.7.2",
    "coveralls": "^2.11.9",
    "nyc": "^6.2.1",
    "proxyquire": "^1.7.4",
    "sinon": "^1.17.3",
    "source-map-support": "^0.4.0",
    "standard": "^6.0.8",
    "tap-difflet": "^0.4.0",
    "zopf": "^4.5.0"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "dependencies": {
    "bluebird": "^3.3.4",
    "file-type": "^3.8.0",
    "read-chunk": "^1.0.1"
  }
}
